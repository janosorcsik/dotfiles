  indentHeuristic = true
  noprefix = true
  renames = copies
[fetch]
  prune = true
  pruneTags = true
  writeCommitGraph = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  process = git-lfs filter-process
  required = true
  smudge = git-lfs smudge -- %f
[help]
  # correct typos
  autoCorrect = 1
[log]
  date = relative
  decorate = full
